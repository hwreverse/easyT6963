   }  

}

void bitMapDemo(){
  lcd.clearGraphic();
  lcd.clearText();

  // Arduino Image 1 + 2
  lcd.drawBitmap(72, 0, Arduino1bmp ,Arduino1HEIGHT, Arduino1BYTEWIDTH );	//x,y = Position, Picturename (not Filename), Height in Pixel, Widht in Byte
